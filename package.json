{
    "name": "csstojs",
    "displayName": "cssToJS",
    "description": "Convert css to js objects for use in React",
    "version": "2.1.0",
    "publisher": "rhystyers",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.convertCSS"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.convertCSS",
                "title": "Convert CSS"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.1.5",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}