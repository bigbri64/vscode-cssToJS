{
    "name": "csstojs",
    "displayName": "cssToJS",
    "description": "Convert css to js objects for use in React",
    "version": "1.0.1",
    "publisher": "rhystyers",
    "author": {
        "name": "Rhys Tyers"
    },
    "icon": "images/logo.png",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Formatters", "Other"
    ],
    "activationEvents": [
        "onCommand:extension.convertCSS"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.convertCSS",
                "title": "Convert CSS"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.1.5",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "license": "https://creativecommons.org/licenses/by/4.0/",
    "bugs": {
        "url": "https://github.com/rhyst/vscode-cssToJS/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rhyst/vscode-cssToJS.git"
    },
    "homepage": "https://github.com/rhyst/vscode-cssToJS/blob/master/README.md"
}
